{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/css/*" ]
  },
  "platform": { "apiRuntime": "dotnet-isolated:7.0" },
  "globalHeaders": {
    "content-security-policy": "base-uri 'self'; object-src 'none'; script-src 'self' 'unsafe-eval'; upgrade-insecure-requests;"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".html": "text/html",
    ".svg": "image/svg+xml",
    ".css": "text/css",
    ".png": "image/png"
  }
}